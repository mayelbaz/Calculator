//************************************************* Validity check ****************************************************//
i                                //non of the following should compile//
i =
i +=
i \
*
9gj = 5
p = 1p
!
i = !
i++
++i
g+i
i = =
i = -
i = 9 +
i = 9 += 5
i = 5 + + 3
5 = 8
g = 8++
i = 2 + 4 / 4 - 5 // 5
+ = 5
//*********************************************** Calculation check1 ****************************************************//
i = 0
j = ++i
x = i++ + 5
y = 5 + 3 * 10
i += y                            // (i=37,j=1,x=6,y=35)

i = 1
j = 2
k = i + j
t = i + j + k                    // i =1 ,j =2 , k= 3, t=6

i = 1
j = 2
k = i - j
t = i - j - k                     // i =1 ,j =2 , k= -1, t=0

i = 1
i = i + i
i += i
k = i++
t = ++i
j = k - t                         // {k=4, j=-2, i=6, t=6}

i = 5
k = ++i + ++i                     // i=7, k= 13

i = 5
k = ++i + i++                     // k =12 i=7
j = i++ + ++i                     // j =16 i =9

i = 6
j = 7
t = 10
k = i++ + ++j * t-- - --t         // i =7 , j = t = 8, k = 78

i = 3
j = i++ + i++ + i++                // i = 6, j = 12

i = 3
i = i++                           // i =3

i = 5
j = i - 3
k = 10 - i - j                    // [{k=3, j=2, i=5}]

i = 3
i -= 4 - 3 * 2                    // i = 5

k = 5 + 4 - 3 * 3                 // k = 0


k = 5
k += 5 + k
y = k++
y = k++ + ++k - --k - k--          //[{k=16, y=0}]

//*********************************************** Calculation check2 ****************************************************//

A = 2
B = 3
C = 4
D = 5
E = 6
a =  A + B - C * D / E
b =  A + B - C / D * E
c =  A + B / C * D - E
d =  A + B / C - D * E
e =  A + B * C - D / E
f =  A + B * C / D - E          [{c=-4, b=5, a=2, E=6, D=5, C=4, B=3, A=2, f=-2, e=14, d=-28}]


